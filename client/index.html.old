                </li>
                </ul>
              </div>
 -->
              <div ng-hide="sessionUser.get('nick')" class="item dropdown">
               <a href="#" class="dropdown-toggle">
                    Login/Signup
                </a>
                <ul  class="dropdown-menu dropdown-menu-right">
                  <li  ui-sref-active="active"><a ui-sref="signup">Sign up</a>
                </li>
                <li  ui-sref-active="active"><a ui-sref="login">Login</a>
                </li>
                </ul>
              </div>
            </div>
            <!-- <div class="meta"> -->
             
              <!-- <div class="breadcrumb-links">
                Home / Dashboard
              </div> -->
            <!-- </div> -->
          </div>
        </div>

        <!-- End Header Bar -->
        
                    
        <!-- Main Content -->
        <section ui-show="!isViewLoading" data-ui-view></section>
      </div><!-- End Page Content -->
