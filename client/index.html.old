<div class="load-modal" ui-hide="!isViewLoading" >
<center>
<popover  >
  <div class="load-modal-inner"><br><br>
  <img class="center-block" height="50px" src="assets/images/loader.gif"/><span><br><br><br><b>Loading...</b></span>
</popover><div></center>
</div>
        
<style>
  .custom-menu-icon{
    position: fixed;
    left:0;
    top:0;
   
    z-index:99;
  } 
  .ui-hide{
    z-index:0;
  }      
  </style>
   
         
                      
   <div id="page-wrapper" ui-show="!isViewLoading"  ng-class="{'active': toggle}" ng-cloak>

    <!-- Sidebar -->

    <div id="sidebar-wrapper"  ng-swipe-left="swipe(0)" ng-swipe-right="swipe(1)">
      <ul class="sidebar" >
        <li class="sidebar-main">
          <a href="#"  ng-click="swipe(0)">
            KJ Master
            <span class="menu-icon glyphicon glyphicon-transfer"></span>
          </a>
        </li>
        <!-- <li class="sidebar-title"><span>NAVIGATION</span></li> -->
        <li ng-repeat="item in menu" class="sidebar-list">
         <a ui-sref="{{item.state}}">{{item.title}}<span class="menu-icon" ng-class="item.icon"></span></a>
        </li>
        <!-- <li class="sidebar-title separator"><span>QUICK LINKS</span></li> -->
       <!--  <li class="sidebar-list">
          <a href="#" target="_blank">Forums <span class="menu-icon fa fa-external-link"></span></a>
        </li> -->
      </ul>
      <div class="sidebar-footer">
        
        <div ng-show="sessionUser" class="col-xs-4">
          <a ui-sref="settings">
            Account
          </a>
        </div>
        <div ng-show="sessionUser" class="col-xs-4">
          <a ui-sref="logout">
            Logout
          </a>
        </div>
         <div ng-hide="sessionUser.get('nick')" class="col-xs-4">
          <a ui-sref="login">
            Login
          </a>
        </div>
        <div ng-hide="sessionUser.get('nick')" class="col-xs-4">
          <a ui-sref="signup">
            Signup
          </a>
        </div>
        <div class="col-xs-4">
           <div ng-show="isAdmin()" ui-sref-active="active"><a ui-sref="admin">Admin</a></div>
                
        </div>
      </div>
    </div>

    <!-- End Sidebar -->
     <div id="content-wrapper">
      <div class="page-content">

        <!-- Header Bar -->

        <div  class="row header">
          <div class="col-xs-12">
            <button class="btn btn-lg btn-warning" ng-click="toggleSidebar()" >
           <span  class="glyphicon glyphicon-th " ></span></button>
            <div  class="pull-right">
             <!-- Single button -->
<div ng-show="sessionUser.get('nick')" class="btn-group item dropdown">
  <button  type="button" class="btn btn-default dropdown-toggle" >
           <span class="fa fa-user"></span> <span class="caret"></span>
  </button>
  <ul ng-show="sessionUser.get('nick')" class="dropdown-menu dropdown-menu-right">
                  <li class="dropdown-header">
                   {{sessionUser.get('nick')}}
                  </li>
                  <li class="link">
                    <a ui-sref="settings">
                      Settings
                    </a>
                  </li>
                </li>
                  <li class="link">
                    <a href="#">
                      History
                    </a>
                  </li>
                  <li class="link">
                    <a href="#">
                      Songbook
                    </a>
                  </li>
                  <li class="divider"></li>
                 <li class="link" ><a ui-sref="logout">Logout</a>
                </li>
                </ul>
</div>
